{
  "name": "csv-hero",
  "version": "0.0.4",
  "description": "CSV Hero will save your day and parse all the CSV files you have",
  "main": "dist/csv-hero.node.js",
  "scripts": {
    "watch:web": "webpack --mode=development --env.target=web --watch --progress",
    "watch:node": "webpack --mode=development --env.target=node --watch --progress",
    "build": "npm run build:web && npm run build:node",
    "build:web": "webpack --mode=production --env.target=web",
    "build:node": "webpack --mode=production --env.target=node"
  },
  "keywords": [
    "csv",
    "parser"
  ],
  "author": "Marius Wieschollek",
  "repository": {
    "type": "git",
    "url": "https://github.com/marius-wieschollek/csv-hero.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
